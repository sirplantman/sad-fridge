{
    "builds": [
        {
            "src": "src/api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "src/(.*)",
            "dest": "api/index.py"
        }
    ]
}